datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
  yfghjs    Yfghj?   @relation(fields: [yfghjsId], references: [id])
  yfghjsId  String?
}

model Yfghj {
  createdAt DateTime @default(now())
  ds        String?
  id        String   @id @default(cuid())
  myUser    User[]
  updatedAt DateTime @updatedAt
}
